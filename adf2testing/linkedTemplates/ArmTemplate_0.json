			"properties": {
				"linkedServiceName": {
					"referenceName": "sql",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "Exe_2 (sql2adl)"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "name",
						"type": "varchar"
